<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>Jdav</servlet-name>
    <servlet-class>cc.co.llabor.JdavServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SimpleWebDav</servlet-name>
    <servlet-class>brix.demo.web.dav.SimpleServlet</servlet-class>
    <init-param>
      <param-name>resource-path-prefix</param-name>
      <param-value>/webdav</param-value>
    </init-param>
    <init-param>
      <param-name>resource-config</param-name>
      <param-value>/WEB-INF/config.xml</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>SimpleWebDav</servlet-name>
    <url-pattern>/webdav/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>JcrWebDav</servlet-name>
    <servlet-class>brix.demo.web.dav.JcrServlet</servlet-class>
    <init-param>
      <param-name>resource-path-prefix</param-name>
      <param-value>/jcrwebdav</param-value>
    </init-param>
  </servlet>
  <servlet>
    <servlet-name>SfWebDav</servlet-name>
    <servlet-class>net.sf.webdav.WebdavServlet</servlet-class>
    <init-param>
      <param-name>rootpath</param-name>
      <param-value>/tmp/davtest/</param-value>
    </init-param>
    <init-param>
      <param-name>ResourceHandlerImplementation</param-name>
      <param-value>cc.co.llabor.dav.MyDav</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>SfWebDav</servlet-name>
    <url-pattern>/sfdav/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>JcrWebDav</servlet-name>
    <url-pattern>/jcrwebdav/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Jdav</servlet-name>
    <url-pattern>/jdav</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>